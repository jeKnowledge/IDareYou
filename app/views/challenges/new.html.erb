<h1>New Challenge</h1>
<%= form_for :challenge, url: challenges_path do |f| %>

	<div><%= f.label :name %><br />
	<%= f.text_field :name %></div>
	<div><%= f.label :description %><br />
	<%= f.text_area :description %></div>
	<div><%= f.label :points %><br />
	<%= f.text_area :points %></div>

	<div><br /><%= f.submit %></div>

<% end %>
